    "nodes": {
        "s1_info": {
            "id": "s1_info",
            "type": "dialogue",
            "speaker": "Zeynep",
            "text": "Sen, Zeynep, gerçekten bunu yapmak için hazır mısın? Bu selzedelere yardım edebilecek misin?",
            "subtitle": "Sen, Zeynep, gerçekten bunu yapmak için hazır mısın? Bu selzedelere yardım edebilecek misin?",
            "image": "",
            "characterImage": "/characters/Bella.png",
            "environment": "Selzede Yardım Merkezinde",
            "next": "s1_d1"
        },
        "s1_d1": {
            "id": "s1_d1",
            "type": "dialogue",
            "speaker": "Brad",
            "text": "Merhaba, ben Zeynep. Selzedelere yardım etmek için buradayım. İlk günüm ve çok heyecanlıyım.",
            "image": "",
            "characterImage": "/characters/Brad.png",
            "environment": "Selzede Yardım Merkezinde",
            "next": "s1_d2"
        },
        "s1_d2": {
            "id": "s1_d2",
            "type": "dialogue",
            "speaker": "Zeynep",
            "text": "Hoş geldin, Zeynep. Seninle birlikte çalışmak bizi çok mutlu etti. Bu selzedelere yardım etmek için çok önemli bir iş yapıyorsun.",
            "image": "",
            "characterImage": "/characters/Ana_1.png",
            "environment": "Selzede Yardım Merkezinde",
            "next": "s1_d3"
        },
        "s1_d3": {
            "id": "s1_d3",
            "type": "dialogue",
            "speaker": "Brad",
            "text": "Teşekkür ederim. Ben de çok heyecanlıyım. Bu selzedelere yardım etmek için ne yapabilirim?",
            "image": "",
            "characterImage": "/characters/Brad.png",
            "environment": "Selzede Yardım Merkezinde",
            "next": "s1_choice"
        },
        "s1_choice": {
